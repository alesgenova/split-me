{
  "name": "split-me",
  "version": "1.1.7",
  "description": "Universal web component to create arbitrary split layouts ",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "docs": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.spec": "stencil test --spec",
    "test.e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "prepublishOnly": "rm -rf .stencil/ dist/ www/ && npm install && npm run build"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.18.0",
    "@types/jest": "^24.0.9",
    "@types/lodash.throttle": "^4.1.5",
    "@types/puppeteer": "1.12.1",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "pixelmatch": "4.0.2",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "puppeteer": "1.12.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alesgenova/split-me.git"
  },
  "author": "Alessandro Genova",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alesgenova/split-me"
  },
  "homepage": "https://github.com/alesgenova/split-me",
  "keywords": [
    "splitter",
    "split-layout",
    "stenciljs",
    "javascript",
    "typescript",
    "webcomponents"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
