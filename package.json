{
  "name": "split-me",
  "version": "0.2.0",
  "description": "Universal web component to create arbitrary split layouts ",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "dependencies": {
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "@stencil/core": "^0.10.2",
    "@types/jest": "^23.1.0",
    "@types/lodash.throttle": "^4.1.3",
    "jest": "^23.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alesgenova/split-me.git"
  },
  "author": "Alessandro Genova",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alesgenova/split-me"
  },
  "homepage": "https://github.com/alesgenova/split-me",
  "keywords": [
    "splitter",
    "split-layout",
    "stenciljs",
    "javascript",
    "typescript",
    "webcomponents"
  ],
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
